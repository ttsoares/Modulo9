{
  "name": "Modulo_7",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "build": "npx tsc && npx typeorm migration:run",
    "start": "node ./dist/server.js",
    "dev": "ts-node-dev src/server.ts",
    "dev:debug": "ts-node-dev --inspect=0.0.0.0:9229 src/server.ts",
    "migration:run": "ts-node-dev ./node_modules/typeorm/cli.js migration:run",
    "migration:run:test": "cross-env NODE_ENV=test ts-node-dev ./node_modules/typeorm/cli.js migration:run",
    "migration:show": "ts-node-dev ./node_modules/typeorm/cli.js migration:show",
    "test:setup": "rimraf testdb.sql && yarn migration:run:test",
    "test": "yarn test:setup && cross-env NODE_ENV=test jest --watchAll --passWithNoTests --silent --noStackTrace --runInBand",
    "test:verbose": "yarn test:setup && cross-env NODE_ENV=test jest --watch --passWithNoTests --runInBand",
    "test:coverage": "yarn test --coverage"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.2",
    "ioredis": "^4.28.3",
    "jsonwebtoken": "^8.5.1",
    "jwt-decode": "^3.1.2",
    "md5": "^2.3.0",
    "pg": "^8.7.1",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.2.41",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/ioredis": "^4.28.7",
    "@types/jest": "^27.4.0",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/md5": "^2.3.2",
    "@types/node": "^17.0.8",
    "@types/supertest": "^2.0.11",
    "@types/uuid": "^8.3.3",
    "cross-env": "^7.0.3",
    "ioredis-mock": "^7.0.0",
    "jest": "^27.4.7",
    "rimraf": "^3.0.2",
    "sqlite3": "^5.0.2",
    "supertest": "^6.2.2",
    "ts-jest": "^27.1.3",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.5.4"
  }
}
